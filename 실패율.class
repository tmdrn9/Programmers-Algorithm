class Solution {
    public int[] solution(int N, int[] stages) {
        int[] answer = new int[N];
        double[] fail = new double[N]; //index+1==stage, value==failure late
        int cnt=stages.length;
        
        for(int i=0;i<stages.length;i++){
            if(stages[i]==N+1)
                continue;
            fail[stages[i]-1]++;
        }
        int temp;
        for(int j=0;j<fail.length;j++){
            if(cnt<=0)
                fail[j]=0;
            else{
                temp=(int)fail[j];
                fail[j]/=cnt;
                cnt-=temp;
            }
            
        }
  
        int index;
        double max;
        for(int m=0;m<fail.length;m++){
            index=0;
            max=-1;
            for(int n=0;n<fail.length;n++){
                if(fail[n]>max){
                    index=n;
                    max=fail[n];
                }
            }
            fail[index]=-1;
            answer[m]=index+1;
        }
      
        return answer;
    }
}