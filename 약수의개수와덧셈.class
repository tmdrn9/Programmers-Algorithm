class Solution {
    public int solution(int left, int right) {
        int answer = 0;
        int cnt=(left==1)?1:2;
        for(int i=left;i<=right;i++){
            for(int j=2;j<i;j++){
                if(i%j==0)
                    cnt++;
            }
            if(cnt%2==0)
                answer+=i;
            else
                answer-=i;
            cnt=2;
        }
        return answer;
    }
}