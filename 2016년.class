class Solution {
    public String solution(int a, int b) {
        String answer = "";
        int days=(a-1)*30+b;
        for(int i=1;i<a;i++){
            if(i<=7){
                if(i==2)
                    days--;
                else{
                    if(i%2==1)
                        days++;
                }
            }
            else{
                if(i%2==0)
                    days++;
            }
        }
        switch(days%7){
            case 0:
                answer="THU";
                break;
            case 1:
                answer="FRI";
                break;
            case 2:
                answer="SAT";
                break;
            case 3:
                answer="SUN";
                break;
            case 4:
                answer="MON";
                break;
            case 5:
                answer="TUE";
                break;
            case 6:
                answer="WED";
                break;
        }
        return answer;
    }
}