class Solution {
    public int[] solution(int[] lottos, int[] win_nums) {
        int[] answer = new int[2];
        int collectCnt=0;
        int nullCnt=0;
        for(int i=0;i<6;i++){
            if(lottos[i]==0){
                nullCnt++;
                continue;
            }
            else{
                for(int j=0;j<6;j++){
                    if(lottos[i]==win_nums[j]){
                        collectCnt++;
                        break;}
                }
            }
        }    
        answer[0]=(collectCnt+nullCnt>1)?(7-collectCnt-nullCnt):6;
        answer[1]=(collectCnt>1)?7-collectCnt:6;
        return answer;
    }
}